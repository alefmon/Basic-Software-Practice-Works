;;;;;;;;;;;;;;				FIBONACCI					;;;;;;;;;;;;;;;;;;
;;;;;;;;;;;;;;											;;;;;;;;;;;;;;;;;;
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
		READ R0
		LOAD R2 A
		LOAD R5 A
		SUB R0 R5
LOOP: 	SUB R0 R5
		JN FIM
		COPY R3 R1
		ADD R1 R2
		COPY R2 R3
		JMP LOOP
FIM: 	WRITE R1
		HALT
A:		WORD 1
		END